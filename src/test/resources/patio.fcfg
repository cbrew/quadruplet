"an": Cat[field=none, cat=det, sem=<\p q.exists x.(p(x) & q(x))>]
Usr[sem=?s] -> Prop[sem=?s] | Attr[sem=?s] | Core[sem=?s] | Ask[sem=?s] | Cmd[sem=?s]
Ask[sem=<Ask(?q)>] -> Whq[q=?q]
Shortans[ans=?s] -> Attr[sem=?s] | Core[sem=?s]
S[sem=<app(?subj, ?vp)>] -> Np[sem=?subj] Vp[sem=?vp]
Np[sem=<app(?det, ?nbar)>] -> Cat[cat=det, sem=?det] Nbar[sem=?nbar]
Nbar[sem=<app(?mod, ?core)>] -> Cat[attr=?attr, sem=?mod] Cat[cat=n, sem=?core]
Nbar[sem=<app(?mod, ?nbar)>] -> Cat[attr=?attr, sem=?mod] Nbar[sem=?nbar]
Nbar[sem=?n] -> Cat[cat=n, sem=?n]
Vp[sem=<app(?v, ?obj)>] -> Cat[cat=tvb, sem=?v] Np[sem=?obj]






